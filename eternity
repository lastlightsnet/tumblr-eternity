	$(document).ready(function() {
	$('.photopost').fancybox({
	padding : 6,
	openEffect  : 'none',
	closeEffect : 'none',

	helpers : {
	title : {
	type : 'inside'
	},
	buttons	: {}
	},

	afterLoad : function() {
	this.title = 'Image ' + (this.index + 1) + ' of ' + this.group.length + (this.title ? ' &nbsp;&nbsp; ' + this.title : '');
	}
	});

	});
</script>

